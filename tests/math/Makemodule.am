# CSV to C struct
$(tests_math_GOLD_SRCS): %.gold.h: %.dat
	@$(MKDIR_P) $(top_builddir)/tests/math/gold
	@rm -f $@
	@echo "$< --> $@"
	@echo "struct gold builtin_gold[] = {" >> $@
	@sed -e 's/,/, /g;s/^/    { /g;s/$$/ },/g;s/0\.00*f/0.0/g;$$s/,$$//g' $< >> $@
	@sed -i 's/\([+-]*[0-9]*\.[0-9]*\)/PCONST(\1)/g' $@
	@echo "};" >> $@

